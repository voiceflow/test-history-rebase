  "main": "src/index.tsx",
  "scripts": {
    "build": "./scripts/build.sh",
    "build:copy-prototype": "cp build/index.html build/prototype.html",
    "build:dev": "SOURCE_MAPS=true yarn g:vite build && yarn build:copy-prototype",
    "build:e2e": "VF_APP_BUILD_ENV=e2e VF_APP_E2E=true yarn g:vite build && yarn build:copy-prototype",
    "clean:build": "rm -rf build",
    "connect:cli": "vfcli mesh connect creator-app -c \"/bin/bash\"",
    "e2e": "yarn kill:e2e && yarn g:run-p e2e:build:watch e2e:preview",
    "e2e:build:watch": "VF_APP_E2E=true VF_APP_BUILD_ENV=e2e yarn g:vite build --watch",
    "e2e:check-deps": "npx wait-on https://localhost:8003/health && npx wait-on https://localhost:8011/health && npx wait-on https://localhost:8005/health && npx wait-on https://localhost:8006/health && npx wait-on https-get://localhost:8013/health",
    "e2e:check-deps-ci": "npx wait-on https://creator-api.test.e2e:8003/health && npx wait-on https://general-runtime.test.e2e:8005/health && npx wait-on https://general-service.test.e2e:8006/health && npx wait-on https-get://realtime.test.e2e:8013/health",
    "e2e:preview": "VF_APP_E2E=true yarn g:vite preview --port 3002",
    "gen-certs:e2e": "scripts/generate_e2e_certs.sh",
    "kill:e2e": "npx kill-port 3002 || true",
    "kill:process": "yarn kill:e2e",
    "lint": "yarn g:run-p -c lint:eslint lint:prettier",
    "lint:eslint": "yarn g:eslint",
    "lint:fix": "yarn g:run-p -c \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier --check",
    "local": "yarn kill:process && vfcli mesh connect creator-app",
    "local:e2e": "yarn kill:e2e && VF_APP_E2E=true VF_APP_BUILD_ENV=e2e yarn g:vite",
    "mesh:post": "exit  0",
    "mesh:pre": "exit 0",
    "start": "NODE_ENV=development yarn g:vite",
    "start:e2e": "scripts/start_e2e.sh",
    "start:hidden": "yarn start --no-open",
    "start:meshed": "scripts/generate_certs.sh && yarn start",
    "start:proxy": "scripts/generate_certs.sh && docker compose up -d proxy",
    "tdd:integration": "yarn g:vitest watch .it",
    "tdd:unit": "yarn g:vitest watch .unit",
    "test": "yarn g:vitest run --silent --coverage",
    "test:dependencies": "yarn g:depcruise --ignore-known",
    "test:integration": "exit 0",
    "test:unit": "REPORTS_DIRECTORY=coverage/unit yarn g:vitest run .unit --silent --coverage",
    "types": "yarn g:tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  },
  "resolutions": {
    "immutable": "3.7.6"
  },
