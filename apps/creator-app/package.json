  "scripts": {
    "analyze:dependencies": "depcruise --exclude src/setupVite.ts --validate -- src",
    "build": "./scripts/build.sh",
    "build:copy-prototype": "cp build/index.html build/prototype.html",
    "build:dev": "SOURCE_MAPS=true vite build && yarn build:copy-prototype",
    "build:e2e": "VF_APP_BUILD_ENV=e2e VF_APP_E2E=true vite build && yarn build:copy-prototype",
    "clean:build": "rm -rf build",
    "connect:cli": "vfcli mesh connect creator-app -c \"/bin/bash\"",
    "cypress": "E2E=true NODE_ENV=test cypress open",
    "cypress:ci": "if npx wait-on --timeout 6000 $CYPRESS_API_URL; then yarn cypress:ci-sorry-cypress; else yarn cypress:ci-cypress; fi",
    "cypress:ci-cypress": "E2E=true NODE_ENV=test cypress run --browser electron --env CI=true --parallel --record --group=\"ci-run\" --ci-build-id=\"$CIRCLE_WORKFLOW_ID\" --config defaultCommandTimeout=20000",
    "cypress:ci-sorry-cypress": "E2E=true NODE_ENV=test cy2 run --env CI=true --parallel --record --group=\"ci-run\" --ci-build-id=\"$CIRCLE_WORKFLOW_ID\" --config defaultCommandTimeout=20000",
    "cypress:install": "cypress cache clear && cypress install && cypress verify",
    "cypress:run": "E2E=true NODE_ENV=test cypress run --browser chrome",
    "e2e": "yarn kill:e2e && concurrently -c yellow,blue \"yarn e2e:build:watch\" \"yarn e2e:preview\"",
    "e2e:build:watch": "VF_APP_E2E=true VF_APP_BUILD_ENV=e2e vite build --watch",
    "e2e:check-deps": "npx wait-on https://localhost:8003/health && npx wait-on https://localhost:8011/health && npx wait-on https://localhost:8005/health && npx wait-on https://localhost:8006/health && npx wait-on https-get://localhost:8013/health",
    "e2e:check-deps-ci": "npx wait-on https://creator-api.test.e2e:8003/health && npx wait-on https://general-runtime.test.e2e:8005/health && npx wait-on https://general-service.test.e2e:8006/health && npx wait-on https-get://realtime.test.e2e:8013/health",
    "e2e:preview": "VF_APP_E2E=true vite preview --port 3002",
    "gen-certs:e2e": "scripts/generate_e2e_certs.sh",
    "kill:e2e": "npx kill-port 3002 || true",
    "kill:process": "yarn kill:e2e",
    "lint": "concurrently -c yellow,blue,magenta,purple \"yarn:lint:js\" \"yarn:lint:css\" \"yarn:lint:dockerfiles\"",
    "lint:css": "stylelint src/**/*.css",
    "lint:dockerfiles": "hadolint --ignore DL3018 ./Dockerfile",
    "lint:fix": "yarn lint:js --fix --rule 'react-hooks/exhaustive-deps: off'",
    "lint:js": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --cache --cache-strategy content",
    "lint:quiet": "concurrently -c yellow,blue \"yarn:lint:js --rule 'prettier/prettier: off' --quiet\" \"yarn:lint:css --quiet\" --color",
    "lint:report": "yarn lint:js --format json -o sonar/report.json && yarn lint:css --formatter json --output-file sonar/report_css.json",
    "local": "yarn kill:process && vfcli mesh connect creator-app",
    "local:e2e": "yarn kill:e2e && VF_APP_E2E=true VF_APP_BUILD_ENV=e2e vite",
    "mesh:post": "exit 0",
    "mesh:pre": "exit 0",
    "start": "NODE_ENV=development vite",
    "start:e2e": "scripts/start_e2e.sh",
    "start:hidden": "yarn start --no-open",
    "start:meshed": "scripts/generate_certs.sh && yarn start",
    "start:proxy": "scripts/generate_certs.sh && docker compose up -d proxy",
    "tdd:integration": "vitest watch .it",
    "tdd:unit": "vitest watch .unit",
    "test": "vitest run --silent --coverage",
    "test:dependencies": "depcheck ",
    "test:integration": "exit 0",
    "test:unit": "REPORTS_DIRECTORY=coverage/unit vitest run .unit --silent --coverage",
    "types": "tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  }
}
