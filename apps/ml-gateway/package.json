{
  "name": "@voiceflow/ml-gateway",
  "version": "1.47.1",
  "dependencies": {
    "@google-cloud/firestore": "6.5.0",
    "@google-cloud/pubsub": "3.5.2",
    "@logux/core": "0.8.1",
    "@logux/server": "0.12.2",
    "@opentelemetry/api-metrics": "0.33.0",
    "@voiceflow/api-sdk": "3.23.27",
    "@voiceflow/backend-utils": "4.8.4",
    "@voiceflow/base-types": "2.84.3",
    "@voiceflow/common": "8.2.0",
    "@voiceflow/exception": "^1.4.0",
    "@voiceflow/logger": "2.0.0",
    "@voiceflow/metrics": "1.4.1",
    "@voiceflow/ml-sdk": "^1.36.0",
    "@voiceflow/sdk-analytics": "1.24.0",
    "@voiceflow/socket-utils": "^1.31.0",
    "@voiceflow/verror": "1.1.3",
    "axios": "1.4.0",
    "body-parser": "1.20.2",
    "chance": "1.1.11",
    "cookie-parser": "1.4.6",
    "core-js": "3.30.2",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "es6-template-string": "^1.2.0",
    "express": "^4.18.1",
    "express-validator": "7.0.1",
    "lodash": "^4.17.21",
    "moize": "6.1.6",
    "nanoevents": "7.0.1",
    "node-fetch": "3.3.1",
    "openai": "^3.2.1",
    "regenerator-runtime": "0.13.11",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "4.17.17"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "analyze:dependencies": "depcruise --exclude src/models.ts --validate -- src",
    "build": "yarn clean && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "clean": "rimraf build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nodemon src/index.ts || true",
    "e2e": "yarn kill:e2e && NODE_ENV=e2e NODE_TLS_REJECT_UNAUTHORIZED=0 yarn start:base",
    "e2e:check-deps": "npx wait-on https://localhost:8003/health ",
    "e2e:check-deps-ci": "npx wait-on https://creator-api.test.e2e:8003/health",
    "e2e:ci": "./scripts/run_e2e_ci.sh",
    "gen-certs:e2e": "scripts/generate_e2e_certs.sh",
    "kill:e2e": "npx kill-port 8015 || true",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:dockerfiles": "hadolint --ignore DL3018 ./Dockerfile",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "local": "vfcli mesh connect ml-gateway",
    "local:e2e": "yarn kill:e2e && NODE_ENV=e2e NODE_TLS_REJECT_UNAUTHORIZED=0 yarn dev",
    "mesh:post": "exit 0",
    "mesh:pre": "exit 0",
    "start": "PORT=8015 CREATOR_API_ENDPOINT=https:// NODE_ENV=production yarn start:base",
    "start:base": "node --experimental-specifier-resolution=node --max-old-space-size=8192 build/index.js",
    "start:meshed": "GOOGLE_APPLICATION_CREDENTIALS=/tmp/voiceflow/ml-gateway/secrets/credentials.json NODE_ENV=local IS_MESHED=true yarn dev",
    "test": "yarn test:unit",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "NODE_ENV=test nyc --report-dir=nyc_coverage_unit ts-mocha --project tsconfig.test.json --paths --config config/test/unit/mocharc.yml",
    "types": "tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  },
  "serviceName": "ml-gateway",
  "type": "module"
}
