{
  "name": "@voiceflow/realtime",
  "version": "1.196.1",
  "dependencies": {
    "@logux/core": "0.8.1",
    "@logux/server": "0.12.2",
    "@opentelemetry/api-metrics": "0.33.0",
    "@voiceflow/alexa-types": "2.14.1",
    "@voiceflow/api-sdk": "3.24.3",
    "@voiceflow/backend-utils": "4.8.4",
    "@voiceflow/base-types": "2.87.2",
    "@voiceflow/common": "8.2.1",
    "@voiceflow/internal": "3.2.0",
    "@voiceflow/logger": "2.0.0",
    "@voiceflow/metrics": "1.4.1",
    "@voiceflow/platform-config": "^1.42.1",
    "@voiceflow/realtime-sdk": "^1.298.0",
    "@voiceflow/slate-serializer": "1.5.2",
    "@voiceflow/socket-utils": "^1.31.0",
    "axios": "1.4.0",
    "bidirectional-adapter": "1.2.1",
    "bson": "4.6.5",
    "core-js": "3.30.2",
    "dayjs": "^1.11.7",
    "dd-trace": "3.20.0",
    "dotenv": "16.0.3",
    "express-validator": "7.0.1",
    "hashids": "2.2.11",
    "lodash": "4.17.21",
    "moize": "6.1.6",
    "mongodb": "3.5.7",
    "normal-store": "1.3.0",
    "polished": "4.2.2",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "regenerator-runtime": "0.13.11",
    "slate": "0.94.0",
    "socket.io": "3.1.2",
    "socket.io-adapter": "2.1.0",
    "socket.io-redis": "6.0.1",
    "typescript-fsa": "3.0.0",
    "unleash-client": "3.19.1",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@types/mongodb": "3.5.18",
    "@types/node": "16.11.41"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "analyze:dependencies": "depcruise --exclude src/models.ts --validate -- src",
    "build": "yarn clean && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "clean": "rimraf build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nodemon src/index.ts || true",
    "e2e": "yarn kill:e2e && NODE_ENV=e2e NODE_TLS_REJECT_UNAUTHORIZED=0 yarn start:base",
    "e2e:check-deps": "npx wait-on https://localhost:8003/health ",
    "e2e:check-deps-ci": "npx wait-on https://creator-api.test.e2e:8003/health",
    "e2e:ci": "./scripts/run_e2e_ci.sh",
    "gen-certs:e2e": "scripts/generate_e2e_certs.sh",
    "kill:e2e": "npx kill-port 8013 || true",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:dockerfiles": "hadolint --ignore DL3018 ./Dockerfile",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint --format json -o sonar/report.json",
    "local": "vfcli mesh connect realtime",
    "local:e2e": "yarn kill:e2e && NODE_ENV=e2e NODE_TLS_REJECT_UNAUTHORIZED=0 yarn dev",
    "mesh:post": "exit 0",
    "mesh:pre": "exit 0",
    "start": "PORT=8013 CREATOR_API_ENDPOINT=https:// NODE_ENV=production yarn start:base",
    "start:base": "node --experimental-specifier-resolution=node --max-old-space-size=8192 build/index.js",
    "start:meshed": "NODE_ENV=local IS_MESHED=true yarn dev",
    "test": "vitest run --coverage",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "REPORTS_DIRECTORY=coverage/unit vitest run .unit --silent --coverage",
    "types": "tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  },
  "serviceName": "realtime",
  "type": "module"
}
