{
  "name": "@voiceflow/showcase",
  "version": "1.52.6",
  "author": "Voiceflow",
  "serviceName": "showcase",
  "bugs": {
    "url": "https://github.com/voiceflow/creator-app/issues"
  },
  "dependencies": {
    "@voiceflow/ui": "^1.267.2",
    "bootstrap": "4.6.1",
    "react": "18.2.0",
    "react-dismissable-layers": "0.3.1",
    "react-dom": "18.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-toastify": "9.1.1",
    "simplebar": "5.3.9",
    "styled-components": "5.3.6"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/voiceflow/creator-app#readme",
  "license": "UNLICENSED",
  "main": "build/index.js",
  "peerDependencies": {
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/creator-app.git"
  },
  "scripts": {
    "analyze:dependencies": "depcruise --validate -- src",
    "build": "./scripts/build.sh",
    "build:dev": "SOURCE_MAPS=true vite build",
    "build:e2e": "VF_APP_BUILD_ENV=e2e VF_APP_E2E=true vite build",
    "clean:build": "rm -rf build",
    "connect:cli": "vfcli mesh connect showcase -c \"/bin/bash\"",
    "e2e": "yarn kill:e2e && concurrently -c yellow,blue \"VF_APP_E2E=true VF_APP_BUILD_ENV=e2e vite build --watch\" \"VF_APP_E2E=true vite preview --port 3004\"",
    "e2e:check-deps": "npx wait-on https://localhost:8005/health ",
    "e2e:check-deps-ci": "exit 0",
    "kill:e2e": "npx kill-port 3005 || true",
    "kill:process": "yarn kill:e2e",
    "lint": "concurrently -c yellow,blue \"yarn:lint:js\" \"yarn:lint:dockerfiles\"",
    "lint:dockerfiles": "hadolint --ignore DL3018 ./Dockerfile",
    "lint:fix": "yarn lint:js --fix",
    "lint:js": "eslint \"**/*.{js,ts,jsx,tsx}\"",
    "lint:quiet": "yarn lint:js --rule 'prettier/prettier: off' --quiet",
    "lint:report": "yarn lint:js --format json -o sonar/report.json",
    "local": "vfcli mesh connect showcase",
    "local:e2e": "yarn kill:e2e && VF_APP_E2E=true VF_APP_BUILD_ENV=e2e vite",
    "mesh:post": "exit 0",
    "mesh:pre": "exit 0",
    "start": "NODE_ENV=development vite",
    "start:e2e": "scripts/start_e2e.sh",
    "start:hidden": "yarn start --no-open",
    "start:meshed": "yarn start",
    "start:proxy": "docker compose up -d proxy",
    "test": "exit 0",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "exit 0",
    "types": "tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  }
}
