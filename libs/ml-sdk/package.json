{
  "name": "@voiceflow/ml-sdk",
  "version": "1.46.0",
  "homepage": "https://github.com/voiceflow/creator-app#readme",
  "bugs": {
    "url": "https://github.com/voiceflow/creator-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/creator-app.git"
  },
  "license": "UNLICENSED",
  "author": "Voiceflow",
  "main": "build/common/index.js",
  "module": "build/module/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "analyze:dependencies": "depcruise --validate -- src",
    "build": "yarn clean && yarn g:run-p build:cjs build:esm",
    "build:cjs": "yarn g:tsc --project tsconfig.build.json && yarn g:tsc-alias -p tsconfig.build.json",
    "build:esm": "yarn g:tsc --project tsconfig.module.json && yarn g:tsc-alias -p tsconfig.module.json",
    "clean": "yarn g:rimraf build",
    "dev": "yarn clean && yarn g:run-p 'yarn:build:cjs --watch' 'yarn:build:esm --watch'",
    "lint": "yarn g:run-p lint:eslint",
    "lint:eslint": "yarn g:eslint \"**/*.{js,ts}\"",
    "lint:fix": "yarn g:run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier . \"!**/*.{js,ts}\"",
    "lint:report": "yarn g:eslint-output \"./src/**/*.{js,ts}\"",
    "test": "yarn g:nyc yarn g:ts-mocha --project tsconfig.test.json --paths --config config/test/mocharc.yml",
    "test:dependencies": "depcheck",
    "test:integration": "exit 0",
    "test:unit": "yarn test",
    "types": "tsc --project tsconfig.build.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@voiceflow/base-types": "2.96.0",
    "@voiceflow/common": "8.2.1",
    "typescript-fsa": "3.0.0"
  }
}
