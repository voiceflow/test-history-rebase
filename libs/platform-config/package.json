{
  "name": "@voiceflow/platform-config",
  "version": "1.60.19",
  "homepage": "https://github.com/voiceflow/creator-app#readme",
  "bugs": {
    "url": "https://github.com/voiceflow/creator-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/creator-app.git"
  },
  "license": "UNLICENSED",
  "author": "Voiceflow",
  "exports": {
    ".": {
      "types": "./build/cjs/index.d.ts",
      "import": "./build/esm/index.js",
      "default": "./build/cjs/index.js"
    },
    "./backend": {
      "types": "./build/backend/index.d.ts",
      "import": "./build/backend/index.mjs",
      "default": "./build/backend/index.mjs"
    }
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "typesVersions": {
    "*": {
      "backend": [
        "./build/backend/index.d.ts"
      ]
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn g:turbo run build:cmd --filter=@voiceflow/platform-config...",
    "build:backend": "yarn g:vite build --ssr ./src/index.ts && yarn g:tsc-alias -p tsconfig.build.esm.json --outDir build/backend",
    "build:cjs": "yarn g:build:pkg cjs",
    "build:cmd": "yarn g:run-p build:cjs build:esm build:backend",
    "build:esm": "yarn g:build:pkg esm",
    "clean": "yarn g:rimraf build",
    "lint": "yarn g:run-p -c lint:eslint lint:prettier",
    "lint:eslint": "yarn g:eslint",
    "lint:fix": "yarn g:run-p -c \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier --check",
    "test": "yarn g:run-p -c test:dependencies test:types",
    "test:dependencies": "yarn g:depcruise",
    "test:types": "yarn g:tsc --noEmit"
  },
  "dependencies": {
    "@voiceflow/alexa-types": "2.14.56",
    "@voiceflow/api-sdk": "3.27.30",
    "@voiceflow/base-types": "2.106.0",
    "@voiceflow/chat-types": "2.13.104",
    "@voiceflow/common": "8.2.8",
    "@voiceflow/dtos": "workspace:*",
    "@voiceflow/google-dfes-types": "2.16.57",
    "@voiceflow/google-types": "2.20.57",
    "@voiceflow/slate-serializer": "1.6.0",
    "@voiceflow/ui": "workspace:*",
    "@voiceflow/voice-types": "2.9.83",
    "@voiceflow/voiceflow-types": "3.27.0",
    "bidirectional-adapter": "1.2.1",
    "normal-store": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.10",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@types/login-with-amazon-sdk-browser": "1.0.3",
    "vite-plugin-dts": "3.9.0"
  },
  "engines": {
    "node": "20"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
