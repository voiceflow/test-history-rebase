{
  "name": "@voiceflow/realtime-sdk",
  "version": "1.384.4",
  "homepage": "https://github.com/voiceflow/creator-app#readme",
  "bugs": {
    "url": "https://github.com/voiceflow/creator-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/creator-app.git"
  },
  "license": "UNLICENSED",
  "author": "Voiceflow",
  "exports": {
    ".": {
      "types": "./build/common/index.d.ts",
      "import": "./build/module/index.js",
      "default": "./build/common/index.js"
    },
    "./backend": {
      "types": "./build/backend/src/index.d.ts",
      "import": "./build/backend/index.mjs",
      "default": "./build/backend/index.js"
    }
  },
  "main": "build/common/index.js",
  "module": "build/module/index.js",
  "typesVersions": {
    "*": {
      "backend": [
        "./build/backend/src/index.d.ts"
      ]
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "yarn g:turbo run build:cmd --filter=@voiceflow/realtime-sdk...",
    "build:cjs": "yarn g:build:pkg cjs",
    "build:cmd": "yarn g:run-p build:cjs build:esm",
    "build:esm": "yarn g:build:pkg esm",
    "lint": "yarn g:run-p -c lint:eslint lint:prettier",
    "lint:eslint": "yarn g:eslint",
    "lint:fix": "yarn g:run-p -c \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier --check",
    "tdd": "yarn g:vitest",
    "test": "yarn g:run-p -c test:dependencies test:types test:unit",
    "test:dependencies": "yarn g:depcruise",
    "test:types": "yarn g:tsc --noEmit",
    "test:unit": "yarn g:vitest run --coverage"
  },
  "dependencies": {
    "@voiceflow/alexa-types": "2.14.56",
    "@voiceflow/base-types": "2.106.0",
    "@voiceflow/chat-types": "2.13.104",
    "@voiceflow/common": "8.2.8",
    "@voiceflow/dtos": "workspace:*",
    "@voiceflow/google-dfes-types": "2.16.57",
    "@voiceflow/google-types": "2.20.57",
    "@voiceflow/internal": "3.2.3",
    "@voiceflow/platform-config": "workspace:*",
    "@voiceflow/schema-types": "1.64.0",
    "@voiceflow/utils-designer": "workspace:*",
    "@voiceflow/voice-types": "2.9.83",
    "@voiceflow/voiceflow-types": "3.27.0",
    "axios": "1.6.0",
    "bidirectional-adapter": "1.2.1",
    "dayjs": "1.11.10",
    "immer": "10.0.1",
    "is-var-name": "2.0.0",
    "lodash": "4.17.21",
    "moize": "6.1.6",
    "normal-store": "1.3.0",
    "recharts": "2.5.0",
    "slate": "0.94.1",
    "ts-pattern": "5.0.5",
    "typescript-fsa": "3.0.0",
    "utility-types": "3.10.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@voiceflow/api-sdk": "3.27.30",
    "cooky-cutter": "1.5.4",
    "draft-js": "0.11.7",
    "polished": "4.2.2",
    "vite-plugin-dts": "3.9.0"
  },
  "peerDependencies": {
    "@voiceflow/api-sdk": "^3",
    "draft-js": "^0.11",
    "polished": "^4"
  },
  "engines": {
    "node": "20"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
