{
  "name": "@voiceflow/sdk-logux-designer",
  "version": "2.0.0",
  "type": "module",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "prebuild": "yarn g:rimraf build",
    "build": "yarn prebuild && yarn g:tsc -p tsconfig.build.json && yarn g:tsc-alias -p tsconfig.build.json && yarn postbuild",
    "postbuild": "touch build/package.json && echo '{ \"type\": \"module\" }' > build/package.json",
    "lint": "yarn g:run-p lint:eslint \"lint:prettier --check\"",
    "lint:eslint": "yarn g:eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "yarn g:run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier . \"!**/*.{js,ts}\"",
    "lint:report": "yarn g:eslint-output \"./src/**/*.{js,ts}\"",
    "test:dependencies": "depcheck",
    "test:types": "yarn g:tsd"
  },
  "prettier": "@voiceflow-meta/prettier-config",
  "eslintConfig": {
    "extends": "@voiceflow-meta/eslint-config"
  },
  "dependencies": {
    "@voiceflow/common": "8.2.0",
    "@voiceflow/internal": "3.2.0",
    "normal-store": "1.3.0"
  },
  "devDependencies": {
    "@voiceflow-meta/eslint-config": "workspace:*",
    "@voiceflow-meta/prettier-config": "workspace:*",
    "@voiceflow-meta/typescript-config": "workspace:*"
  },
  "stableVersion": "1.35.0",
  "tsd": {
    "directory": "src"
  }
}
