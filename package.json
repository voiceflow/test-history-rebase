{
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*",
      "meta/*",
      "test/*"
    ]
  },
  "scripts": {
    "analyze:dependencies": "turbo run analyze:dependencies --concurrency=4",
    "build": "turbo run build --concurrency=4",
    "build:all": "yarn build",
    "build:creator-app": "cd apps/creator-app; yarn build",
    "build:deps": "yarn build --filter @voiceflow/ui --filter @voiceflow/platform-config --filter @voiceflow/socket-utils --filter @voiceflow/realtime-sdk --filter @voiceflow/dtos",
    "build:ml-gateway": "cd apps/ml-gateway; yarn build",
    "build:platform-config": "cd libs/platform-config; yarn build",
    "build:realtime": "cd apps/realtime; yarn build",
    "build:realtime-http": "cd apps/realtime; yarn build",
    "build:realtime-sdk": "cd libs/realtime-sdk; yarn build",
    "build:services": "yarn build --filter @voiceflow/realtime --filter @voiceflow/ml-gateway",
    "build:socket-utils": "cd apps/socket-utils; yarn build",
    "build:ui": "cd  libs/ui; yarn build",
    "build:utils-designer": "cd  libs/utils-designer; yarn build",
    "clean": "yarn clean:artifacts && yarn clean:root",
    "clean:artifacts": "turbo run clean --concurrency=4",
    "clean:root": "yarn g:rimraf node_modules",
    "codegen": "turbo run codegen --concurrency=4",
    "codegen:release": "turbo run codegen:release",
    "commit": "cz",
    "connect:cli": "cd apps/creator-app; yarn connect:cli",
    "dev": "turbo run dev --parallel",
    "e2e": "cd apps/creator-app; yarn e2e",
    "e2e:check-deps": "cd apps/creator-app; yarn e2e:check-deps",
    "e2e:check-deps-ci": "cd apps/creator-app; yarn e2e:check-deps-ci",
    "g:eslint": "cd $INIT_CWD && eslint",
    "g:hadolint": "hadolint --ignore DL3018 ./Dockerfile",
    "g:nyc": "cd $INIT_CWD && nyc",
    "g:openapi:validate": "cd $INIT_CWD && $PROJECT_CWD/node_modules/.bin/vfpkg openapi validate-schema",
    "g:orval": "cd $INIT_CWD && orval --config ./orval.config.ts",
    "g:prettier": "cd $INIT_CWD && prettier --ignore-path ../../.prettierignore --loglevel error",
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:run-p": "cd $INIT_CWD && run-p",
    "g:schema:copy": "cp ./apps/$SERVICE/openapi.next.json $INIT_CWD/openapi.json",
    "g:ts-mocha": "cd $INIT_CWD && ts-mocha",
    "g:tsc": "cd $INIT_CWD && tsc",
    "g:tsc-alias": "cd $INIT_CWD && tsc-alias",
    "g:tsd": "cd $INIT_CWD && tsd",
    "g:tsx": "cd $INIT_CWD && tsx",
    "g:vite": "cd $INIT_CWD && vite",
    "g:vitest": "cd $INIT_CWD && vitest",
    "g:depcheck": "cd $INIT_CWD && depcheck",
    "g:depcruise": "cd $INIT_CWD && depcruise",
    "g:depcruise-baseline": "cd $INIT_CWD && depcruise-baseline",
    "g:clean-package": "cd $INIT_CWD && clean-package",
    "g:eslint-output": "cd $INIT_CWD && eslint-output",
    "gen-certs:e2e": "turbo run gen-certs:e2e",
    "kill:e2e": "cd apps/creator-app; yarn kill:e2e",
    "lint": "turbo run lint --concurrency=4",
    "lint:commit": "yarn commit",
    "lint:hadolint": "turbo run lint:hadolint --concurrency=4",
    "lint:dockerfiles": "yarn lint:hadolint",
    "lint:fix": "turbo run lint:fix --concurrency=4",
    "lint:report": "turbo run lint:report",
    "local": "cd apps/creator-app; yarn local",
    "local:creator-app": "cd apps/creator-app; yarn local",
    "local:e2e": "yarn g:run-p local:e2e:creator-app local:e2e:realtime",
    "local:e2e:creator-app": "cd apps/creator-app; yarn local:e2e",
    "local:e2e:realtime": "cd apps/realtime; yarn local:e2e",
    "local:ml-gateway": "cd apps/ml-gateway; yarn local",
    "local:realtime": "cd apps/realtime; yarn local",
    "local:realtime-http": "cd apps/realtime; yarn local:realtime-http",
    "local:realtime:all": "npx concurrently -n realtime,realtime-http 'yarn local:realtime' 'yarn local:realtime-http'",
    "mesh:post:creator-app": "cd apps/creator-app; yarn mesh:post",
    "mesh:post:ml-gateway": "cd apps/ml-gateway; yarn mesh:post",
    "mesh:post:realtime": "cd apps/realtime; yarn mesh:post:realtime",
    "mesh:post:realtime-http": "cd apps/realtime; yarn mesh:post:realtime-http",
    "mesh:pre:creator-app": "cd apps/creator-app; yarn mesh:pre",
    "mesh:pre:ml-gateway": "cd apps/ml-gateway; yarn mesh:pre",
    "mesh:pre:realtime": "cd apps/realtime; yarn mesh:pre:realtime",
    "mesh:pre:realtime-http": "cd apps/realtime; yarn mesh:pre:realtime-http",
    "prepare": "husky install",
    "start": "cd apps/creator-app; yarn start",
    "start:e2e": "cd apps/creator-app; yarn start:e2e",
    "start:hidden": "cd apps/creator-app; yarn start:hidden",
    "start:meshed": "cd apps/creator-app; yarn start:meshed",
    "start:meshed:creator-app": "cd apps/creator-app; yarn start:meshed",
    "start:meshed:ml-gateway": "cd apps/ml-gateway; yarn start:meshed",
    "start:meshed:realtime": "cd apps/realtime; yarn start:meshed:realtime",
    "start:meshed:realtime-http": "cd apps/realtime; yarn start:meshed:realtime-http",
    "start:proxy": "cd apps/creator-app; yarn start:proxy",
    "test": "turbo run test",
    "test:dependencies": "turbo run test:dependencies --concurrency=4",
    "test:integration": "turbo run test:integration",
    "test:run": "turbo run test:run",
    "test:unit": "turbo run test:unit --concurrency=2",
    "types": "turbo run types"
  },
  "commitlint": {
    "extends": [
      "@voiceflow/commitlint-config"
    ]
  },
  "prettier": "@voiceflow/prettier-config",
  "resolutions": {
    "@liaoliaots/nestjs-redis@9.0.5": "patch:@liaoliaots/nestjs-redis@npm:9.0.5#.yarn/patches/@liaoliaots-nestjs-redis-npm-9.0.5-0e54717410.patch",
    "@logux/core@0.8.5": "patch:@logux/core@npm:0.8.5#.yarn/patches/@logux-core-npm-0.8.5-b98b5f3a70.patch",
    "@logux/server@0.12.5": "patch:@logux/server@npm:0.12.5#.yarn/patches/@logux-server-npm-0.12.5-1fb079276c.patch",
    "@mikro-orm/core@5.9.4": "patch:@mikro-orm/core@npm:5.9.4#.yarn/patches/@mikro-orm-core-npm-5.9.4-930f83c41a.patch",
    "immutable": "3.7.6",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@types/deep-equal-in-any-order": "1.0.3",
    "@types/mock-require": "2.0.1",
    "@vitest/coverage-c8": "0.31.0",
    "@voiceflow/babel-preset": "1.1.1",
    "@voiceflow/commitlint-config": "2.1.0",
    "@voiceflow/dependency-cruiser-config": "1.3.1",
    "@voiceflow/eslint-config": "7.1.0",
    "@voiceflow/git-branch-check": "1.4.1",
    "@voiceflow/prettier-config": "1.3.0",
    "@voiceflow/semantic-release-config": "1.2.0",
    "@voiceflow/tsconfig": "1.4.8",
    "@voiceflow/vite-config": "1.11.0",
    "clean-package": "^2.2.0",
    "deep-equal-in-any-order": "2.0.6",
    "dependency-cruiser": "12.12.1",
    "eslint": "8.40.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "mock-require": "3.0.3",
    "node-dev": "8.0.0",
    "nodemon": "2.0.22",
    "npm-run-all": "^4.1.5",
    "nyc": "15.1.0",
    "orval": "6.17.0",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "ts-loader": "9.4.2",
    "ts-mocha": "10.0.0",
    "tsc-alias": "1.8.6",
    "turbo": "1.10.16",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-node": "0.31.0",
    "vitest": "0.31.0"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "20"
  },
  "volta": {
    "node": "20.12.1",
    "yarn": "3.2.1"
  }
}
